  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "files.exclude": {
    "**/.git": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.excludeLanguages": [
    "react-jsx",
    "typescript",
    "typescriptreact",
    "markdown"
  ],
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
